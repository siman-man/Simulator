<div id="node_status" class="panel left_panel">
  <header>
    <h3 class="header_title">Node情報</h3>
  </header>
  <form class="form-horizontal" role="form">
    <div class="form-group">
      <label class="col-sm-2 control-label">ID</label>
      <div class="col-sm-8">
        <input id="user_eid" class="form-control" readonly>
      </div>
    </div>
    <div class="form-group">
      <label for="inputPassword" class="col-sm-2 control-label">Name</label>
      <div class="col-sm-8">
        <input id="user_name" class="form-control">
      </div>
    </div>
    <select id="move_model" class="selectpicker">
      <optgroup label="MoveModel">
        <option value="0" selected>RandomWayPoint</option>
        <option value="1">MapRouteMovement</option>
        <option value="2">RandomWalk</option>
        <option value="3">StationaryMovement</option>
      </optgroup>
    </select>
    <div class="checkbox">
      <label>
        <input type="checkbox" id="create_route"> 経路作成
      </label>
    </div>
    <input type="number" id="wait_time" min="0" max="100" value="0">

    <div id="user_speed">
      <form id="reservation">
        <label for="minbeds">ユーザスピード</label>
        <select name="minbeds" id="minbeds">
          <option>1</option>
          <option>2</option>
          <option>3</option>
          <option>4</option>
          <option selected>5</option>
          <option>6</option>
          <option>7</option>
          <option>8</option>
          <option>9</option>
          <option>10</option>
        </select>
      </form>
    </div>
  </form>
</div>