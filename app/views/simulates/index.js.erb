<% @obj_list.each do |obj| %>
	var x, y;
	console.log("create =>");
	var opt = <%= obj.to_json.html_safe %>
	x = <%= obj[:x] %>
	y = <%= obj[:y] %>
	<% case obj[:type] %>
	<% when :server %>
		Node.create(x, y, opt);
	<% when :start %>
		Node.create( x, y, opt);
	<% when :end %>
		Node.create( x, y, opt);
	<% when :user %>
		Node.create(x, y, opt);
	<% when :wall %>
		Wall.create(x, y);
	<% when :road %>
		Street.create(x, y);
	<% when :lake %>
		Lake.create(x, y);
	<% when :car %>
		Node.create(x, y, opt);
	<% else %>	
	<% end %>
<% end %>

